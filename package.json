{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build:rspack": "rspack build -c rspack.config.js",
    "start:rspack": "node dist/main.js",
    "start:webpack": "npx node dist/server.js",
    "build:webpack": "npx webpack --config webpack-hmr.config.js"
  },
  "dependencies": {
    "@nestjs/common": "9.3.12",
    "@nestjs/core": "9.3.12",
    "@nestjs/platform-express": "9.3.12",
    "@rspack/cli": "^0.1.9",
    "aws4": "^1.12.0",
    "mongoose": "6.4.6",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.1",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.12",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "run-script-webpack-plugin": "^0.1.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.9.5",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
